{
  "name": "ember-power-calendar-moment",
  "version": "0.2.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon",
    "ember-power-calendar-adapter"
  ],
  "repository": "github:cibernox/ember-power-calendar-moment",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@embroider/macros": "^1.13.1",
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.9",
    "@babel/plugin-proposal-decorators": "^7.22.7",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.0",
    "@embroider/test-setup": "^3.0.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.0",
    "ember-cli": "~4.12.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-power-calendar": "git+https://git@github.com/cibernox/ember-power-calendar.git#80b06b08dbbb07e3ab025f7d18bb2887e22faf76",
    "ember-qunit": "^7.0.0",
    "ember-resolver": "^11.0.0",
    "ember-source": "~4.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.1",
    "ember-try": "^2.0.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.10.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-qunit": "^8.0.0",
    "loader.js": "^4.7.0",
    "moment": "^2.29.4",
    "prettier": "^3.0.1",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "sass": "^1.64.2",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.2",
    "webpack": "^5.88.2"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0 || >=5.0.0",
    "moment": "^2.0.0",
    "moment-timezone": "^0.5.43"
  },
  "peerDependenciesMeta": {
    "moment": {
      "optional": true
    },
    "moment-timezone": {
      "optional": true
    }
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
